<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <title>Título da página</title>
    <meta charset="utf-8">
    <script>
        let n = parseInt(prompt('Digite um número entre 0 e 100:'))

        if (n >= 0 && n <= 100) {

            if (n >= 0 && n <= 25) {
                document.write('Intervalo [0,25]')
            }
            if (n > 25 && n <= 50) {
                document.write('Intervalo [25,50]')
            }
            if (n > 50 && n <= 75) {
                document.write('Intervalo [50,75]')
            }
            if (n > 75 && n <= 100) {
                document.write('Intervalo [75, 100]')
            }
        } else {
            document.write('Fora de intervalo.')
        }

    </script>
  </head>
  <body>
    
  </body>
</html>
